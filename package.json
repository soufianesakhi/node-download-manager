{
  "name": "node-download-manager",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --prod",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "watch-ng": "ng build --watch",
    "build-server": "tsc -p src/tsconfig.server.json",
    "watch-server": "nodemon -w src/server -e ts --exec \"npm run build-server && title node && set PORT=5000 && node dist-server/main\"",
    "build-ext": "tsc -p src/tsconfig.ext.json",
    "watch-ext": "title \"tsc ext\" && tsc --watch -p src/tsconfig.ext.json",
    "watch-scripts": "title scripts && tsc --watch -p src/tsconfig.scripts.json",
    "rix": "node dist-scripts/scripts/reindexer.js",
    "l": "start npm run watch-ng& start npm run watch-server&",
    "le": "start npm run watch-ext&",
    "ls": "start npm run watch-scripts&"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.2.0-rc.0",
    "@angular/cdk": "^8.1.2",
    "@angular/common": "^8.2.0-rc.0",
    "@angular/compiler": "^8.2.0-rc.0",
    "@angular/core": "^8.2.0-rc.0",
    "@angular/forms": "^8.2.0-rc.0",
    "@angular/http": "^7.2.15",
    "@angular/platform-browser": "^8.2.0-rc.0",
    "@angular/platform-browser-dynamic": "^8.2.0-rc.0",
    "@angular/router": "^8.2.0-rc.0",
    "body-parser": "^1.18.2",
    "core-js": "^2.4.1",
    "express": "^4.16.2",
    "mime-types": "^2.1.26",
    "mongoose": "^5.0.6",
    "node-fetch": "^2.6.0",
    "node-notifier": "^5.4.3",
    "request": "^2.83.0",
    "request-progress": "^3.0.0",
    "rxjs": "^6.5.2",
    "rxjs-compat": "^6.0.0-rc.0",
    "sanitize-filename": "^1.6.1",
    "source-map-support": "^0.5.3",
    "su-downloader3": "^1.1.1",
    "tslib": "^1.9.0",
    "unirest": "^0.5.1",
    "websocket": "^1.0.25",
    "wretch": "^1.7.1",
    "xlsx": "^0.12.2",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.801.3",
    "@angular/cli": "^8.1.3",
    "@angular/compiler-cli": "^8.2.0-rc.0",
    "@angular/language-service": "^8.2.0-rc.0",
    "@types/body-parser": "^1.16.8",
    "@types/express": "^4.11.1",
    "@types/jasmine": "~2.8.3",
    "@types/jasminewd2": "~2.0.2",
    "@types/jquery": "3.2.1",
    "@types/mongodb": "^3.0.5",
    "@types/mongoose": "^5.0.2",
    "@types/node": "~6.0.60",
    "@types/node-notifier": "0.0.28",
    "@types/request": "^2.47.0",
    "@types/websocket": "0.0.37",
    "codelyzer": "^5.0.1",
    "jasmine-core": "~2.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~2.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "nodemon": "^1.15.0",
