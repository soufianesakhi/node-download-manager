{
  "name": "node-download-manager",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --prod",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "watch-ng": "ng build --watch",
    "build-server": "tsc -p src/tsconfig.server.json",
    "watch-server": "nodemon -w src/server -e ts --exec \"npm run build-server && title node && node dist-server/main\"",
    "build-ext": "tsc -p src/tsconfig.ext.json",
    "watch-ext": "title \"tsc ext\" && tsc --watch -p src/tsconfig.ext.json",
    "watch-scripts": "title scripts && tsc --watch -p src/tsconfig.scripts.json",
    "l": "start npm run watch-ng& start npm run watch-server&",
    "le": "start npm run watch-ext&",
    "ls": "start npm run watch-scripts&"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^5.2.0",
    "@angular/common": "^5.2.0",
    "@angular/compiler": "^5.2.0",
    "@angular/core": "^5.2.0",
    "@angular/forms": "^5.2.0",
    "@angular/http": "^5.2.0",
    "@angular/platform-browser": "^5.2.0",
    "@angular/platform-browser-dynamic": "^5.2.0",
    "@angular/router": "^5.2.0",
    "body-parser": "^1.18.2",
    "core-js": "^2.4.1",
    "express": "^4.16.2",
    "mongoose": "^5.0.6",
    "node-notifier": "^5.2.1",
    "request": "^2.83.0",
    "request-progress": "^3.0.0",
    "rxjs": "^5.5.6",
    "sanitize-filename": "^1.6.1",
    "source-map-support": "^0.5.3",
    "unirest": "^0.5.1",
    "websocket": "^1.0.25",
    "xlsx": "^0.12.2",
    "zone.js": "^0.8.19"
  },
  "devDependencies": {
    "@angular/cli": "~1.7.0",
    "@angular/compiler-cli": "^5.2.0",
    "@angular/language-service": "^5.2.0",
    "@types/body-parser": "^1.16.8",
    "@types/express": "^4.11.1",
    "@types/jasmine": "~2.8.3",
    "@types/jasminewd2": "~2.0.2",
    "@types/jquery": "3.2.1",
    "@types/mongodb": "^3.0.5",
    "@types/mongoose": "^5.0.2",
    "@types/node": "~6.0.60",
    "@types/node-notifier": "0.0.28",
    "@types/request": "^2.47.0",
    "@types/websocket": "0.0.37",
    "codelyzer": "^4.0.1",
    "jasmine-core": "~2.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~2.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "nodemon": "^1.15.0",
    "protractor": "~5.1.2",
    "ts-node": "~4.1.0",
    "tslint": "~5.9.1",
    "typescript": "~2.5.3"
  }
}